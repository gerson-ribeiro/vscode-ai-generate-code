{
  "name": "ai-generate-code",
  "displayName": "AI Generate Code",
  "publisher": "gerson-ribeiro",
  "description": "",
  "version": "1.0.0",
  "icon": "extension-logo.jpg",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/gerson-ribeiro/vscode-ai-generate-code"
  },
  "engines": {
    "vscode": "^1.88.0"
  },
  "capabilities": {
    "virtualWorkspaces": true
  },
  "extensionDependencies": [],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "menus": {
      "activityBar": [
        {
          "id": "ai-code",
          "title": "AI Generate Code",
          "group": "ai-code",
          "submenu": [
            {
              "id": "constant-submenu",
              "title": "Generate File from Constant",
              "submenu": [
                {
                  "title": "Generate File with Constant Value",
                  "command": "extension.generateFile"
                }
              ]
            }
          ]
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ai-code",
          "title": "AI Generate Code",
          "icon": "./media/icon.svg"
        }
      ]
    },
    "views": {
      "ai-code": [
        {
          "id": "ai-code.chat",
          "name": "AI Generate Code - Chat",
          "type": "webview"
        }
      ]
    },
    "configuration": [
      {
        "title": "ai-configurations-key",
        "properties": {
          "ai-generate-code.apikey": {
            "type": "string",
            "description": "Api Key from OpenAI chatbot"
          }
        }
      },
      {
        "title": "ai-configurations-model",
        "properties": {
          "ai-generate-code.model": {
            "type": "string",
            "description": "Model from OpenAI chatbot"
          }
        }
      }
    ]
  },
  "files": [
    "src/assets/templates/chat/index.html",
    "src/assets/templates/chat/index.css",
    "src/assets/templates/chat/index.js"
  ],
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/react": "^18.2.79",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "agentkeepalive": "^4.5.0",
    "axios": "^1.6.8",
    "highlight": "^0.2.4",
    "highlight.js": "^11.9.0",
    "markdown-it": "^14.1.0",
    "openai": "^4.38.1",
    "react": "^18.2.0",
    "whatwg-url": "^14.0.0"
  }
}
